{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "shell",
            "label": "gcc build active file",
            "command": "/usr/bin/gcc",
            "args": [
                "-g",
                "${file}",
                "-I",
                "/usr/include/",
                "-L",
                "/usr/lib/",
                "-lssl",
                "-lcrypto",
                "-levent",
                "-levent_openssl",
                "-lpthread",
                "-lcjson",
                "-lm",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "/usr/bin"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build"
        },
